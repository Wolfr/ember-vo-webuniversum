<div class="contact-card__content">
  <div class="contact-card__data">
      <dl>
        {{yield (hash
          address=(component 'contact-card-content-address' contactInfo=contactInfo)
          website=(component 'contact-card-content-website' contactInfo=contactInfo)
          telephone=(component 'contact-card-content-telephone' contactInfo=contactInfo)
          fax=(component 'contact-card-content-fax' contactInfo=contactInfo)
          email=(component 'contact-card-content-email' contactInfo=contactInfo)
          row=(component 'contact-card-content-row')
          )}}
      </dl>
    </div>
</div>
