<div class="date-range">
  <div class="date-range__group">
    <label for="datepicker-from" class="date-range__label">{{fromLabel}}</label>
    <div class="date-range__datepicker datepicker">
      {{wu-datepicker-input
        class="input-field input-field--block"
        rawValue=fromValue
        placeholder=fromPlaceholder
        minDate=fromMinDate
        maxDate=fromMaxDate
        dateFormat=dateFormat
        disabled=disabled
        onChange=onChangeFromValue
      }}
    </div>
  </div>

  <div class="date-range__group">
    <label for="datepicker-to" class="date-range__label">{{toLabel}}</label>
    <div class="date-range__datepicker datepicker">
      {{wu-datepicker-input
        class="input-field input-field--block"
        rawValue=toValue
        placeholder=toPlaceholder
        minDate=toMinDate
        maxDate=toMaxDate
        dateFormat=dateFormat
        disabled=disabled
        onChange=onChangeToValue
      }}
    </div>
  </div>
</div>
{{yield}}
